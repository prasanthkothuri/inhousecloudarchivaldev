{
    "env": "dev",
    "connections": {
        "onprem_orcl_conn": "onprem_orcl_conn"
    },
    "warehouses": {
        "dev_warehouse": "s3://natwest-data-archive-vault"
    },
    "retention_policies": {
        "default_6m": "6m"
    },
    "sources": [
        {
            "name": "on_prem_orcl",
            "connection": "onprem_orcl_conn",
            "include": {
                "schemas": [
                    {
                        "name": "EDI_SUP_OWNER",
                        "tables": [
                            {
                                "name": "FEED",
                                "condition": "FEED_ID < 10000"
                            },
                            {
                                "name": "SDAS_DOCUMENT_DETAILS_LOB"
                            },
                            {
                                "query": "SELECT * FROM EDI_SUP_OWNER.BANK_HOLIDAYS WHERE TERRITORY = 'MIB_UNITED_KINGDOM'",
                                "target_table": "EDI_SUP_OWNER_BANK_HOLIDAYS"
                            }
                        ]
                    }
                ]
            },
            "exclude": {},
            "warehouse": "dev_warehouse",
            "retention_policy": "default_6m",
            "legal_hold": false
        }
    ]
}
