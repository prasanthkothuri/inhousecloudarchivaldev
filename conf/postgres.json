{
    "env": "dev",
    "connections": {
        "pg_doc_db_metadata_conn": "pg_doc_db_metadata_conn"
    },
    "warehouses": {
        "dev_warehouse": "s3://natwest-data-archive-vault"
    },
    "retention_policies": {
        "default_6m": "6m"
    },
    "sources": [
        {
            "name": "pg_doc_db_metadata",
            "connection": "pg_doc_db_metadata_conn",
            "include": {
                "schemas": [
                    {
                        "name": "doc_sup_owner",
                        "tables": [
                            {
                                "name": "application_reference_data"
                            },
                            {
                                "name": "bank_holidays"
                            },
                            {
                                "name": "feed"
                            },
                            {
                                "name": "feed_batch"
                            },
                            {
                                "name": "feed_dependencies"
                            }
                        ]
                    }
                ]
            },
            "exclude": {},
            "warehouse": "dev_warehouse",
            "retention_policy": "default_6m",
            "legal_hold": false
        }
    ]
}
